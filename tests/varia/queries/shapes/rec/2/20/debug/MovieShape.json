{
	"name": "MovieShape",
	"targetDef":{
		"query":"SELECT ?x WHERE {GRAPH <dbpedia-person.org> {?x a dbo:Film FILTER (?x = dbr:Johnny_Lingo)}}"
	},
	"constraintDef":{
		"conjunctions":[
			[ 	
				{ "path": "dbo:director", "min": 1, "value":"dbr:Judge_Whitaker"},
				{ "path": "dbo:writer", "min": 1, "value":"dbr:Patricia_McGerr"},
				{ "path": "dbo:imdbId", "min": 1 },
				{ "path": "dbo:imdbId", "max": 1 },
				{ "path": "dbo:starring", "max": 0, "shape":"ActorShape","negated":true}
			]
		]
	}
}
