{
	"name": "Movie",
	"targetDef":{
		"query":"SELECT ?x WHERE {?x a dbo:Film FILTER (?x = <http://dbpedia.org/resource/Titanic_(1997_film)>)}"
	},
	"constraintDef":{
		"conjunctions":[
			[ 	
				{ "path": "dbo:director", "min": 1, "shape": "DirectorShape" },
				{ "path": "dbo:imdbId", "min": 1 },
				{ "path": "dbo:imdbId", "max": 1 },
				{ "path": "dbo:starring", "min": 1},
				{ "path": "dbo:director", "min": 1, "value":"<http://dbpedia.org/resource/James_Cameron>"}
			]
		]
	}
}
