PREFIX dbo:<http://dbpedia.org/ontology/>
PREFIX dbr:<http://dbpedia.org/resource/>
SELECT ?x1 WHERE{
GRAPH <dbpedia-person-20pct.org>{

?x1 a dbo:Film.

FILTER NOT EXISTS {
 
?x1 dbo:starring ?x2.
?x1 dbo:imdbId ?id1.
?x1 dbo:writer ?w1.
?x1 dbo:musicComposer ?c1.
?x1 dbo:director ?x4.
 
FILTER NOT EXISTS {
?x1 dbo:imdbId ?n_11.
?x1 dbo:imdbId ?n_12.
FILTER(?n_11 != ?n_12)
}


?x2 dbo:activeYearsStartYear ?y2 .
?x2 dbo:birthPlace ?x3.
?x2 dbo:imdbId ?i2.
?x2 dbo:occupation dbr:Actor .
?x2 ^dbo:starring ?m2.
 

?x3 dbo:country ?c3.
?x3 dbo:leaderTitle ?l3.
?x3 dbo:populationDensity ?d3.


?x4 dbo:imdbId ?i4.
?x4 ^dbo:director ?d4.

FILTER NOT EXISTS{

?x4 dbo:birthPlace ?x5.

FILTER NOT EXISTS{
?x5 dbo:country ?c5.
?x5 dbo:leaderTitle ?l5.
?x5 dbo:populationDensity ?d5.
 
}
}
}
}}
