{
	"name": "MovieShape",
	"targetDef":{
		"query":"SELECT ?x WHERE {GRAPH <dbpedia-person.org> {?x a dbo:Film}}"
	},
	"constraintDef":{
		"conjunctions":[
			[ 	
				{ "path": "dbo:director", "min": 1},
				{ "path": "dbo:imdbId", "min": 1 },
				{ "path": "dbo:imdbId", "max": 1 },
				{ "path": "dbo:starring", "max": 0, "shape":"ActorShape","negated":true}
			]
		]
	}
}
