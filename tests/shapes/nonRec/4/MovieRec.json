{
	"name": "MovieRec",
	"targetDef":{
		"query":"SELECT ?x WHERE {?x a dbo:Film}"
	},
	"constraintDef":{
		"conjunctions":[
			[ 	
				{ "path": "dbo:director", "min": 1, "shape": "DirectorShape" },
				{ "path": "dbo:imdbId", "min": 1 },
				{ "path": "dbo:starring", "max": 0,  "shape": "ActorShape", "negated":true}
			]
		]
	}
}
