{
	"name": "Movie",
	"targetDef":{
		"query":"SELECT ?x WHERE {GRAPH <dbpedia-person.org> {?x a dbo:Film}}"
	},
	"constraintDef":{
		"conjunctions":[
			[ 	
				{ "path": "dbo:director", "min": 1, "shape": "DirectorShape" },
				{ "path": "dbo:imdbId", "min": 1 },
				{ "path": "dbo:imdbId", "max": 1 },
				{ "path": "dbo:starring", "min": 1, "shape":"ActorShape"}
			]
		]
	}
}
